<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.shachiko.weeklytask</string>

    <key>ProgramArguments</key>
    <array>
        <string>/opt/miniconda3/bin/python</string>
        <string>/Users/shachiko/Desktop/shachiko-alarm/alarm.py</string>
    </array>

<!-- time:毎週火曜１６時４４分 -->
    <key>StartCalendarInterval</key>
    <dict>
        <key>Weekday</key>
        <integer>2</integer> 
        <key>Hour</key>
        <integer>16</integer>
        <key>Minute</key>
        <integer>44</integer>
    </dict>


    <key>StandardOutPath</key>
    <string>/tmp/weeklytask.out</string>
    <key>StandardErrorPath</key>
    <string>/tmp/weeklytask.err</string>

    <key>RunAtLoad</key>
    <false/>
</dict>
</plist>